package JavaBasics;

import java.util.ArrayList;

public class ArrayListConcept {

	public static void main(String[] args) {

		ArrayList al = new ArrayList();
		al.add(100); //0
		al.add(200); //1
		al.add(300); //2
		al.add(12.25); //3
		al.add('A'); //4
		al.add("Ajay"); //5
		al.add("Singh"); //6
		System.out.println(al.size());
		System.out.println(al.get(4));
		
	}

}
